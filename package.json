{
  "name": "meteor-4pi",
  "version": "1.4.1",
  "author": "Daan van der Munnik (https://github.com/grmbl99)",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "test": "meteor test --once --driver-package meteortesting:mocha",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "spell-interact": "mdspell --en-us --ignore-numbers --ignore-acronyms '**/*.md' '!node_modules/**/*.md' '!CHANGELOG.md'",
    "lint:spell": "mdspell --en-us -n -a -r '**/*.md' '!node_modules/**/*.md' '!CHANGELOG.md'",
    "lint:md": "markdownlint '**/*.md'",
    "lint:es": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:ts": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@types/meteor": "^2.0.1",
    "@types/node": "^17.0.19",
    "@types/react": "^17.0.38",
    "@types/react-datepicker": "^4.3.4",
    "@types/react-dom": "^17.0.9",
    "azure-devops-node-api": "^11.1.0",
    "date-fns": "^2.28.0",
    "meteor-node-stubs": "^1.1.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-datepicker": "^4.6.0",
    "react-dnd": "^14.0.5",
    "react-dnd-html5-backend": "^14.1.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.5",
    "typescript": "^4.5.4"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.jsx",
      "server": "server/main.js"
    }
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "markdown-spellcheck": "^1.3.1",
    "markdownlint-cli": "^0.30.0",
    "prettier": "^2.5.1",
    "semantic-release": "^18.0.1",
    "sinon": "^12.0.1"
  },
  "release": {
    "branches": [
      "main"
    ],
    "npmPublish": false,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# Meteor-4PI"
        }
      ],
      "@semantic-release/github",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ]
        }
      ]
    ]
  }
}
